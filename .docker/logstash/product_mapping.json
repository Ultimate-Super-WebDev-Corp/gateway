{
  "template": "product",
  "settings": {
    "index.refresh_interval": "5s"
  },
  "mappings": {
    "properties": {
      "brand": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword"
          }
        }
      },
      "id": {
        "type": "long"
      },
      "name": {
        "type": "text"
      },
      "rating": {
        "type": "long"
      },
      "votes": {
        "type": "long"
      }
    }
  }
}
